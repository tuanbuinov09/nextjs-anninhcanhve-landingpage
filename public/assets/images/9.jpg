<script>
// Attach a submit handler to the form
jQuery( "#formLienhe" ).submit(function( event ) {
  event.preventDefault();
	jQuery("input[type=submit]").val('Đang gửi...');
  var form = jQuery( this );
  var  datacc = form.serialize();
	//alert(datacc);
  url = form.attr( "action" );
	
  var posting = jQuery.post( url, { data: datacc } );
 
  posting.done(function( result ) {
	  console.log(result);
	  if(result == 'success'){
		jQuery("input[type=text]").val(' ');
		jQuery("textarea").val(' ');
		jQuery('.send_cc').html("<p class='txt_white'>Thành Công! Cám ơn bạn đã gửi liên hệ cho chúng tôi!</p>");
	  }else{
		  jQuery('.send_cc').html("<p class='txt_white'>Đã xảy ra lỗi! Xin vui lòng thử lại sau!</p>");
	  }
    
  });
});
</script>
